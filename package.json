{
  "name": "sinkdrummer2016",
  "version": "0.1.0",
  "description": "sinkdrummer in web audio",
  "main": "sinkdrummer.js",
  "scripts": {
    "server": "./node_modules/http-server/bin/http-server",
    "build": "./node_modules/browserify/bin/cmd.js sinkdrummer.js --debug -o bundle.js -t [ babelify --presets [ react ] ]",
    "watch":   "./node_modules/watchify/bin/cmd.js sinkdrummer.js --debug -o bundle.js -t [ babelify --presets [ react ] ] &",
    "watch-stop": "kill $(ps | grep watchify | grep -v grep | awk '{ print $1 }')"
  },
  "author": "Kevin Cox <kevintcox@gmail.com>",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "async": "^1.5.0",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "http-server": "^0.8.5",
    "watchify": "^3.6.1"
  }
}
